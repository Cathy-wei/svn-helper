{
  "name": "svn-helper-crm",
  "version": "2.3.0",
  "description": "svn record helper",
  "type": "module",
  "main": "index.js",
  "bin": {
    "crm3": "bin/index.js"
  },
  "engines": {
    "node": ">= 16.7.0"
  },
  "scripts": {
    "preview": "node ./bin/index.js",
    "dev": "node index.mjs",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "staged": "lint-staged"
  },
  "keywords": [
    "svn"
  ],
  "author": "yunhao",
  "license": "ISC",
  "repository": "cloudhao1999/svn-helper",
  "dependencies": {
    "inquirer": "^8.0.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "husky": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js}": [
      "prettier --write"
    ]
  }
}
